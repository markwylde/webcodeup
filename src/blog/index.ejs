<%- await include('./components/head.html') -%>
<%- await include('./components/header.html', {active: 'blog'}) -%>

<main>
  <article>
    <h1>Blog</h1>
    <p>Here are the latest blog entries.</p>

    <ul>
    <% blogEntries.forEach(entry => { %>
      <li>
        <%= format(new Date(entry.created.authorTime), 'MMM, yyyy') %>
        -
        <a href="blog/<%= entry.id %>"><%= entry.title %></a>
      </li>
    <% }) %>
    </ul>
  </article>
</main>

<%- await include('./components/footer.html') -%>
<%- await include('./components/foot.html') -%>
